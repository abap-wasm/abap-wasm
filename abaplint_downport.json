{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true,
    "useApackDependencies": false
  },
  "dependencies": [
    {
      "folder": "/deps",
      "files": "/**/*.*"
    }
  ],
  "syntax": {
    "version": "v702",
    "errorNamespace": ""
  },
  "rules": {
    "downport": true,
    "check_syntax": true,
    "keyword_case": true,
    "indentation": true,
    "definitions_top": true,
    "parser_error": true
  }
}
